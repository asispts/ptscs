{
    "name": "asispts/ptscs",
    "description": "PSR-12 coding standard with some strict rules",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Asis Pattisahusiwa",
            "email": "asis.pattisahusiwa@gmail.com"
        }
    ],
    "keywords": [
        "standard",
        "standards",
        "phpcs",
        "coding",
        "style"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ptscs\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "slevomat/coding-standard": "^8.8",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.14|^9.5|^10.0"
    }
}
