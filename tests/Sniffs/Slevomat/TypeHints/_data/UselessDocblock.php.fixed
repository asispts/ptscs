<?php declare(strict_types=1);

namespace App;

final class Foobar
{
    /**
     * @var string
     */
    private $value;

    /**
     * Class constructor.
     */
    public function __construct(string $value)
    {
        $this->value = $value;
    }

    public function getValue(): string
    {
        return $this->value;
    }
}
